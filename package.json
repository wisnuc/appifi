{
  "name": "appifi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node build/app.js",
    "build": "babel src --out-dir build",
    "devel": "NODE_ENV=test nodemon --ignore out --ignore tmptest --ignore remote src/app.js --standalone --fruitmix-only --mdns",
    "devel-cloud": "nodemon --ignore out --ignore tmptest src/app.js --fake-bootstrap --disable-smb",
    "devel-dev": "NODE_ENV=dev nodemon --ignore out --ignore tmptest --ignore remote src/app.js --fake-bootstrap --disable-smb",
    "devel-bootstrap": "NODE_ENV=test nodemon --ignore out --ignore tmptest --ignore remote src/app.js --fake-bootstrap --disable-smb",
    "devel-bootstrap-smb": "NODE_ENV=test nodemon --ignore out --ignore tmptest src/app.js --fake-bootstrap",
    "devel-assets": "nodemon --exec babel-node --ignore web --ignore public/bundle.js assets.js",
    "devel-appstore": "nodemon --exec babel-node --ignore web --ignore public/bundle.js src/app.js --appstore-master",
    "webpack": "webpack --watch --watch-poll",
    "unit": "node_modules/.bin/mocha --compilers js:babel-core/register --recursive test",
    "fruit-devel": "PORT=3721 nodemon --exec babel-node --ignore tmptest src/fruitmix/main.js --path tmptest",
    "fruit-family": "PORT=3721 nodemon --ignore 'family/*' --exec babel-node src/fruitmix/main.js --family",
    "fruit-unit": "NODE_PATH=. mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/unit",
    "fruit-algo": "NODE_PATH=. NODE_ENV=test mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/algo",
    "fruit-agent": "NODE_PATH=. NODE_ENV=test mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/agent",
    "mocha": "NODE_PATH=. NODE_ENV=test nodemon --ignore tmptest --ignore out --exec mocha --recursive --sort",
    "lyc": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --compilers js:babel-core/register",
    "lycmon": "NODE_ENV=test nodemon --exec nyc --reporter=lcov --reporter=text mocha --compilers js:babel-core/register",
    "doc": "nodemon --ignore out --ignore tmpest --exec 'jsdoc -c jsdoc.conf.json -t node_modules/ink-docstrap/template -r -p src src/README.md'",
    "baseline": "nodemon --ignore out --ignore tmpest --exec 'jsdoc -c jsdoc.conf.json -t node_modules/jsdoc-baseline -r -p src src/README.md'",
    "sidekick": "node src/fruitmix/sidekick/cluster"
  },
  "dependencies": {
    "@ronomon/crypto-async": "^2.2.1",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "1.17.2",
    "canonical-json": "0.0.4",
    "cookie-parser": "~1.4.3",
    "crypt3": "^1.0.0",
    "cuint": "^0.2.2",
    "debug": "~3.0.1",
    "deep-equal": "^1.0.1",
    "deep-freeze": "0.0.1",
    "dicer": "^0.2.5",
    "express": "~4.15.4",
    "file-type": "^6.1.0",
    "form-data": "^2.3.1",
    "fs-xattr": "^0.2.0",
    "get-args": "^2.0.0",
    "ioctl": "^2.0.1",
    "jwt-simple": "^0.5.1",
    "line-by-line": "^0.1.5",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "morgan": "~1.8.2",
    "mqtt": "^2.14.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^3.0.0",
    "passport-strategy": "^1.0.0",
    "path-to-regexp": "^2.2.1",
    "read-chunk": "^2.1.0",
    "request": "^2.85.0",
    "rimraf": "^2.6.1",
    "sanitize-filename": "^1.6.1",
    "socket.io-client": "^2.0.3",
    "superagent": "^3.6.0",
    "transmission": "^0.4.9",
    "ursa": "^0.9.4",
    "uuid": "^3.1.0",
    "validator": "^8.1.0",
    "webtorrent": "^0.98.20",
    "winston": "^3.0.0-rc4",
    "xxhash": "^0.2.4",
    "xxhashjs": "^0.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.1",
    "image-size": "^0.6.1",
    "ink-docstrap": "^1.3.2",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.11.0",
    "jsdoc": "^3.5.5",
    "jsdoc-baseline": "https://github.com/hegemonic/baseline/tarball/master",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.17.4",
    "nyc": "^11.1.0",
    "raw-loader": "^0.5.1",
    "sinon": "^3.2.1",
    "sloc": "^0.2.0",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.5"
  },
  "wisnuc": {
    "ubuntu-server-amd64": {
      "prerequisites": {
        "node": "8.5",
        "debs": [
          "libimage-exiftool-perl"
        ]
      }
    }
  }
}
